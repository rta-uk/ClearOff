{
  "manifest_version": 3,
  "name": "Clear Off",
  "version": "1.0",
  "description": "Group all tabs except the current one and Google Meet tabs",
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Clear Off"
  },
  "background": {
    "service_worker": "background.js"
  }
}